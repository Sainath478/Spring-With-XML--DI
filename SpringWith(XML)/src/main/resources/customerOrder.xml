<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="cus" class="com.demo.Customer">
		<constructor-arg index="0" value="Shivam Jha"></constructor-arg>
		<constructor-arg index="1" value="online(PhonePay)"></constructor-arg>
		<constructor-arg index="2" ref="order"></constructor-arg>
	</bean>


	<bean id="order" class="com.demo.Order">
		<constructor-arg index="0" value="23659"></constructor-arg>
		<constructor-arg index="1" value="Iron machine"></constructor-arg>
		<constructor-arg index="2" value="08-12-2020"></constructor-arg>
	</bean>








</beans>